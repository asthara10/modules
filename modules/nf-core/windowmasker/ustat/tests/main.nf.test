nextflow_process {

    name "Test Process WINDOWMASKER_USTAT"
    script "../main.nf"
    config "./nextflow.config"
    process "WINDOWMASKER_USTAT"

    tag "modules"
    tag "modules_nfcore"
    tag "windowmasker"
    tag "windowmasker/ustat"

    test("test_windowmasker_ustat") {


        when {
            process {
                """
                
                input[0] = file(params.test_data['sarscov2']['genome']['genome_fasta'], checkIfExists: true)

    WINDOWMASKER_MKCOUNTS ( [ [id:'test'], input ] )

                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out.versions).match("versions") }
                { assert snapshot(process.out).match() }
            )
        }

    }

}
